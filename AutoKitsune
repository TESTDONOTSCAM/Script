spawn(function()
    while wait() do
	if _G.Timer then
        pcall(function()
            if game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149431" then
				if not game:GetService("Workspace").Map:FindFirstChild("KitsuneIsland") then
					local minutes = 6
					local seconds = 1

					repeat
						if seconds <= 0 then
							minutes = minutes - 1
							seconds = 59
						else
							seconds = seconds - 1
						end
						if seconds < 9 then
							Kitsune.Text = "Full Moon 100% ["..tostring(minutes)..":0"..tostring(seconds).."] Until Hop Server"
						else
							Kitsune.Text = "Full Moon 100% ["..tostring(minutes)..":"..tostring(seconds).."] Until Hop Server"
						end
						wait(1)
						until minutes <= 0 and seconds <= 0 or not _G.Timer
						_G.CheckHop = true
					end
            	end
        	end)
		end
    end
end)

spawn(function()
    while wait() do
		if _G.CheckHop then
			pcall(function()
				if game:GetService("Workspace").Map:FindFirstChild("KitsuneIsland") then
					_G.Timer = false
					_G.Kitsune = true
					_G.SpamAzure = true
				elseif not game:GetService("Workspace").Map:FindFirstChild("KitsuneIsland") then
					Hop()
            	end
        	end)
		end
    end
end)

