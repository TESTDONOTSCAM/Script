             Button.BackgroundTransparency = 1.000
             Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
             Button.BorderSizePixel = 0
             Button.Size = UDim2.new(0, 289, 0, 33)
            
            UICorner_2.CornerRadius = UDim.new(0, 4)
            UICorner_2.Parent = Button
            
            UIPadding_2.Parent = Button
            UIPadding_2.PaddingLeft = UDim.new(0, 10)
            
            TextBtn.Name = "Button"
            TextBtn.Parent = Button
            TextBtn.AnchorPoint = Vector2.new(0.5, 0.5)
            TextBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextBtn.BackgroundTransparency = 1.000
            TextBtn.BorderColor3 = Color3.fromRGB(0, 0, 0)
            TextBtn.BorderSizePixel = 0
            TextBtn.Position = UDim2.new(0.5, 0, 0.5, 0)
            TextBtn.Selectable = false
            TextBtn.Size = UDim2.new(0, 270, 0, 33)
            TextBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextBtn.TextXAlignment = Enum.TextXAlignment.Left
            
            TextBtnStroke.Name = "TextBtnStroke"
            TextBtnStroke.Parent = Button
            TextBtnStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TextBtnStroke.Color = _G.StrokeColor
            TextBtnStroke.LineJoinMode = Enum.LineJoinMode.Round
            TextBtnStroke.Thickness = 1.5
            TextBtnStroke.Transparency = 0
