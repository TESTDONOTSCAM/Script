function DoubleQ()
	local Lv = game:GetService("Players").LocalPlayer.Data.Level.Value
	if _G.Upto then
		Lv = Lv + 100
	end
	if World1 and not Auto_Raid then
		if Lv == 1 or Lv <= 9 or SelectMonster == "" then -- Bandit
			Ms = "Bandit"
			NameQuest = "BanditQuest1"
			QuestLv = 1
			NameMon = "Bandit"
