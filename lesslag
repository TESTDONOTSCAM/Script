getgenv().LessLag = {
task.spawn(function ()
    while task.wait() do
        pcall(function ()
            if getgenv().LessLag == true then
                loadstring(game:HttpGet('https://raw.githubusercontent.com/TESTDONOTSCAM/Script/main/ScriptBlox-LessLag'))()
            elseif getgenv().LessLag == false then
                loadstring(game:HttpGet('https://raw.githubusercontent.com/Script-Blox/Script/main/Script-Blox'))()
            end
        end)
    end
end)
}
