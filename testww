TextBtn.MouseEnter:Connect(function()
   TweenService:Create(
    Black,
    TweenInfo.new(0.4,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
    {
     BackgroundTransparency = 0.7
    }
   ):Play()
   end)
  TextBtn.MouseLeave:Connect(function()
   TweenService:Create(
    Black,
