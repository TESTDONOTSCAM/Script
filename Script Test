TEST:Toggle("Remove Ship",_G.RemoveShip,function(va)
	_G.RemoveShip = va
end)

spawn(function()
	while wait() do
		if _G.RemoveShip then
			pcall(function()
				local P32 = game.workspace:WaitForChild("Boats"):FindFirstChild("Guardian") if P32 then P32:Destroy() end
				wait(3)
				_G.RemoveShip = false
			end)
		end
	end
end)

TEST:Toggle("Auto Saiwwl",false,function(val)
_G.AutoSail = val
end)

if _G.AutoSail == true then

BuyBoat = TPP(CFrame.new(-16206.9014, 7.75759554, 404.552338, -0.0348494053, 0, 0.999392569, 0, 1, 0, -0.999392569, 0, -0.0348494053))
if (CFrame.new(-16206.9014, 7.75759554, 404.552338, -0.0348494053, 0, 0.999392569, 0, 1, 0, -0.999392569, 0, -0.0348494053).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
if BuyBoat then BuyBoat:Stop() end
	local args = {
	[1] = "BuyBoat",
	[2] = "Guardian"
	}	
	game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))

	wait(1)
	_G.AutoSail = false
	_G.AutoShip = true
	end
end

spawn(function()
	while wait() do
		pcall(function()
			if _G.AutoShip then

			game.Workspace.Gravity = 0
			game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(15,false)
			game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(16,false)

					if game:GetService("Workspace").Boats:FindFirstChild("Guardian") then
						if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
						TPP(game:GetService("Workspace").Boats.Guardian.VehicleSeat.CFrame * CFrame.new(0,1,0))
					elseif game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == true then
						repeat wait()
							if (game:GetService("Workspace").Boats.Guardian.VehicleSeat.CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
								TPB(CFrame.new(-43186.0938, 20.755722, 14150.7451, 0.404593438, -0.026889408, 0.914101243, 0.0663140193, 0.9977988, 5.95448357e-10, -0.912089169, 0.06061773, 0.405485988))
							end
					until game:GetService("Workspace").SeaBeasts:FindFirstChild("SeaBeast1") or game:GetService("Workspace").Enemies:FindFirstChild("Shark") or game:GetService("Workspace").Enemies:FindFirstChild("Piranha") or game:GetService("Workspace").Enemies:FindFirstChild("PirateBrigade") or _G.Auto_Seabest == false
					end
				end
			end
		end)
	end
end)

TEST:Toggle("Ghost Ship",false,function(val)
	_G.Bridage = val
	StopTween(_G.Bridage)
end)

spawn(function()
	while wait() do
		if _G.Bridage then
			   pcall(function()
				 for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
					 if v:FindFirstChild("HumanoidRootPart") then
					 elseif game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == true then
						game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = false
						AutoHaki()
						EquipWeapon(_G.Setting_table.Weapon)
						topos(v.HumanoidRootPart.CFrame * CFrame.new(0,300,0))
						game:GetService("VirtualUser"):CaptureController()
						game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
					else
						if game:GetService("ReplicatedStorage"):FindFirstChild("Piranha") or game:GetService("ReplicatedStorage"):FindFirstChild("PirateBrigade") or game:GetService("ReplicatedStorage"):FindFirstChild("Shark") then
						topos(game:GetService("ReplicatedStorage"):FindFirstChild("Piranha") or game:GetService("ReplicatedStorage"):FindFirstChild("PirateBrigade") or game:GetService("ReplicatedStorage"):FindFirstChild("Shark").HumanoidRootPart.CFrame * CFrame.new(0,300,0))
						else	
						topos(CFrame.new(-43186.0938, 20.755722, 14150.7451, 0.404593438, -0.026889408, 0.914101243, 0.0663140193, 0.9977988, 5.95448357e-10, -0.912089169, 0.06061773, 0.405485988))
						if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
							game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = true
							end
						end
					end
				end
			end)
		end
	end
end)

TEST:Toggle("SeaBeast",false,function(sea)
	_G.Beast = sea
	StopTween(_G.Beast)
end)

spawn(function()
	while wait() do
		if _G.Beast then
			   pcall(function()
				 for i,v in pairs(game:GetService("Workspace").SeaBeasts:GetChildren()) do
					 if v:FindFirstChild("HumanoidRootPart") then
					 elseif game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == true then
						game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = false
						AutoHaki()
						EquipWeapon(_G.Setting_table.Weapon)
						topos(v.HumanoidRootPart.CFrame * CFrame.new(0,300,0))
						game:GetService("VirtualUser"):CaptureController()
						game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
					else
						if game:GetService("ReplicatedStorage"):FindFirstChild("SeaBeast1") then
						topos(game:GetService("ReplicatedStorage"):FindFirstChild("SeaBeast1").HumanoidRootPart.CFrame * CFrame.new(0,300,0))
						else	
						topos(CFrame.new(-43186.0938, 20.755722, 14150.7451, 0.404593438, -0.026889408, 0.914101243, 0.0663140193, 0.9977988, 5.95448357e-10, -0.912089169, 0.06061773, 0.405485988))
						if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
							game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = true
							end
						end
					end
				end
			end)
		end
	end
end)
