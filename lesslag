getgenv().LessLag = {}

spawn(function ()
    while wait() do
        pcall(function ()
            if getgenv().LessLag then
                loadstring(game:HttpGet('https://raw.githubusercontent.com/Script-Blox/Script/main/Script-Blox'))()
            else
                loadstring(game:HttpGet('https://raw.githubusercontent.com/Script-Blox/Script/main/Keyboard'))()
            end
        end)
    end
end)
