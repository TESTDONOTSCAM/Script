	Frametime.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
	Frametime.BorderSizePixel = 0
	Frametime.Position = UDim2.new(0.5, 0, -0.03, 0)
	Frametime.Size = UDim2.new(0, 240, 0, 40)
	
	UICorner214.CornerRadius = UDim.new(0, 4)
