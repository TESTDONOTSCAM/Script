local Library = Update:Window("Blox Fruits", "")

local Main = Library:Tab("        Main","rbxassetid://14764164264",_G.ColorMain)
local Egg = Library:Tab("        Egg","rbxassetid://14764164264",_G.ColorEgg)
local Teleport = Library:Tab("        Teleport","rbxassetid://14764164264",_G.ColorTeleport)

do

MapSelect = {
   "Area [1]",
   "Area [2]",
   "Area [3]"
}

Main:Seperator("Auto Farm",_G.ColorMain)

Main:Dropdown("Select Map",_G.ColorMain,MapSelect,function(vu)
   _G.SelectMap = vu
end)

if _G.SelectMap == nil then
   _G.SelectMap = "Area [1]"
end

Main:Toggle("Auto Farm",_G.ColorMain,false,function(vu)
   _G.AutoFarm = vu
end)

spawn(function()
   while wait() do
      if _G.AutoFarm then
         pcall(function()
            PetMap()
            if _G.SelectMap == _G.SelectMap then
               TP1(MapWalk1)
               wait(5)
               TP1(MapWalk2)
            end
         end)
      end
   end
end)

Main:Seperator("",_G.ColorMain)

Main:Toggle("Auto Random Potions",_G.ColorMain,false,function(vu)
   _G.AutoPotion = vu
end)

spawn(function()
   while wait() do
      if _G.AutoPotion then
         pcall(function()
           game:GetService("ReplicatedStorage").Network:WaitForChild("VendingMachines_Purchase"):InvokeServer("PotionVendingMachine1",1)
         end)
      end
   end
end)

Main:Toggle("Auto Random Enchants",_G.ColorMain,false,function(vu)
   _G.AutoPotion = vu
end)

spawn(function()
   while wait() do
      if _G.AutoPotion then
         pcall(function()
           game:GetService("ReplicatedStorage").Network:WaitForChild("VendingMachines_Purchase"):InvokeServer("EnchantVendingMachine1",1)
         end)
      end
   end
end)


   
Main:Toggle("Auto Claim Diamond",_G.ColorMain,false,function(vu)
   _G.ClaimDiamond = vu
end)

spawn(function()
   while wait() do
      if _G.ClaimDiamond then
         pcall(function()
           game:GetService("ReplicatedStorage").Network:WaitForChild("DailyRewards_Redeem"):InvokeServer("SmallDailyDiamonds")
         end)
      end
   end
end)

end

