                wait(0.5)
                ScriptLibrary.Enabled = false
                else
                hide = false
                ScriptLibrary.Enabled = true
                Main:TweenSize(UDim2.new(0, 600, 0, 350),"Out","Quad",0.5,true)
            end
        end
    end)

