						local ICIHs = WaterFall:FindFirstChild("Tree") if ICIHs then ICIHs:Destroy() end
						--local ICIMSS = WaterFall:FindFirstChild("Model") if ICIMSS then ICIMSS:Destroy() end
						local ICIMssS = WaterFall:FindFirstChild("Details") if ICIMssS then ICIMssS:Destroy() end
						local WaHouse = WaterFall:FindFirstChild("BigHouse") if WaHouse then WaHouse:Destroy() end
						local WaBam = WaterFall:FindFirstChild("BambooTree") if WaBam then WaBam:Destroy() end
						local WaCir = WaterFall:FindFirstChild("Circle") if WaCir then WaCir:Destroy() end
						local WaPart = WaterFall:FindFirstChild("Part") if WaPart then WaPart:Destroy() end
						local WaGrass = WaterFall:FindFirstChild("PluginGrass") if WaGrass then WaGrass:Destroy() end
						local WaGrasss = WaterFall:FindFirstChild("snowisland_Cylinder.081") if WaGrasss then WaGrasss:Destroy() end
						local WaGrassa = WaterFall:FindFirstChild("Wedge") if WaGrassa then WaGrassa:Destroy() end
					
						-- Turtle
						local Turtle = game.Workspace.Map.Turtle.IslandModel
					
						local ICIMd = Turtle:FindFirstChild("Rocks") if ICIMd then ICIMd:Destroy() end
						local ICIMda = Turtle:FindFirstChild("Tree") if ICIMda then ICIMda:Destroy() end
						local TurShroom = Turtle:FindFirstChild("Shroom1") if TurShroom then TurShroom:Destroy() end
						local TurShroom2 = Turtle:FindFirstChild("Shroom2") if TurShroom2 then TurShroom2:Destroy() end
						local TurVine1 = Turtle:FindFirstChild("Vines1") if TurVine1 then TurVine1:Destroy() end
						local TurVine2 = Turtle:FindFirstChild("Vines2") if TurVine2 then TurVine2:Destroy() end
						local TurVine3 = Turtle:FindFirstChild("Vines3") if TurVine3 then TurVine3:Destroy() end
						local TurDetails = Turtle:FindFirstChild("Town2Details") if TurDetails then TurDetails:Destroy() end
						local TurTown = Turtle:FindFirstChild("TownDetails") if TurTown then TurTown:Destroy() end   
						local TurTrees = Turtle:FindFirstChild("Trees") if TurTrees then TurTrees:Destroy() end
						local TurRock1 = Turtle:FindFirstChild("Rock1") if TurRock1 then TurRock1:Destroy() end
						local TurPart = Turtle:FindFirstChild("Part") if TurPart then TurPart:Destroy() end
						local TurRock = Turtle:FindFirstChild("Rock") if TurRock then TurRock:Destroy() end
						local TurWood = Turtle:FindFirstChild("Wood") if TurWood then TurWood:Destroy() end
						local TurWoods = Turtle:FindFirstChild("PluginGrass") if TurWoods then TurWoods:Destroy() end
						print("Cleaning Map")
					end
				end
			end
		end)
	end)

