local Animation = false

Egg:Button("Remove Egg Animation",_G.ColorEgg,function()
   if Animation == false then
   Animation = true
   game:GetService("Players").LocalPlayer.PlayerGui.EggOpenAnimation.Enabled = false
   else
   Animation = false
   game:GetService("Players").LocalPlayer.PlayerGui.EggOpenAnimation.Enabled = true
   end
end)

Egg:Dropdown("Select Egg",_G.ColorEgg,HatchEgg,function(vu)
   _G.SelectEgg = vu
end)

if _G.SelectEgg == nil then
   _G.SelectEgg = "Cracked Egg"
end

Egg:Dropdown("Hatch Multiple Egg",_G.ColorEgg,MultipleEgg,function(vu)
   _G.MultiEgg = vu
end)

if _G.MultiEgg == nil then
   _G.MultiEgg = "1 Hatch Egg"
end

Egg:Toggle("Auto Hatch Egg",_G.ColorEgg,false,function(vu)
   _G.AutoHatch = vu
   if _G.AutoHatch == true then
      repeat wait()
      if _G.MultiEgg == "1 Hatch Egg" then
         _G.OpenMore = 1
      elseif _G.MultiEgg == "2 Hatch Egg" then
         _G.OpenMore = 2
      elseif _G.MultiEgg == "3 Hatch Egg" then
         _G.OpenMore = 3
      elseif _G.MultiEgg == "4 Hatch Egg" then
         _G.OpenMore = 4
      elseif _G.MultiEgg == "5 Hatch Egg" then
         _G.OpenMore = 5
      elseif _G.MultiEgg == "6 Hatch Egg" then
         _G.OpenMore = 6
      elseif _G.MultiEgg == "7 Hatch Egg" then
         _G.OpenMore = 7
      elseif _G.MultiEgg == "8 Hatch Egg" then
         _G.OpenMore = 8
      elseif _G.MultiEgg == "9 Hatch Egg" then
         _G.OpenMore = 9
      elseif _G.MultiEgg == "10 Hatch Egg" then
         _G.OpenMore = 10
      elseif _G.MultiEgg == "11 Hatch Egg" then
         _G.OpenMore = 11
      elseif _G.MultiEgg == "12 Hatch Egg" then
         _G.OpenMore = 12
      elseif _G.MultiEgg == "13 Hatch Egg" then
         _G.OpenMore = 13
      elseif _G.MultiEgg == "14 Hatch Egg" then
         _G.OpenMore = 14
      elseif _G.MultiEgg == "15 Hatch Egg" then
         _G.OpenMore = 15
      elseif _G.MultiEgg == "16 Hatch Egg" then
         _G.OpenMore = 16
      elseif _G.MultiEgg == "17 Hatch Egg" then
         _G.OpenMore = 17
      elseif _G.MultiEgg == "18 Hatch Egg" then
         _G.OpenMore = 18
      elseif _G.MultiEgg == "19 Hatch Egg" then
         _G.OpenMore = 19
      elseif _G.MultiEgg == "20 Hatch Egg" then
         _G.OpenMore = 20
      elseif _G.MultiEgg == "21 Hatch Egg" then
         _G.OpenMore = 21
      elseif _G.MultiEgg == "22 Hatch Egg" then
         _G.OpenMore = 22
      elseif _G.MultiEgg == "23 Hatch Egg" then
         _G.OpenMore = 23
      elseif _G.MultiEgg == "24 Hatch Egg" then
         _G.OpenMore = 24
      elseif _G.MultiEgg == "25 Hatch Egg" then
         _G.OpenMore = 25
      elseif _G.MultiEgg == "26 Hatch Egg" then
         _G.OpenMore = 26
      elseif _G.MultiEgg == "27 Hatch Egg" then
         _G.OpenMore = 27
      elseif _G.MultiEgg == "28 Hatch Egg" then
         _G.OpenMore = 28
      elseif _G.MultiEgg == "29 Hatch Egg" then
         _G.OpenMore = 29
      elseif _G.MultiEgg == "30 Hatch Egg" then
         _G.OpenMore = 30
      end
      until not _G.AutoHatch
   end
end)

spawn(function()
   while wait() do
      if _G.AutoHatch then
         pcall(function()
               local args = {
                  [1] = _G.SelectEgg,
                  [2] = _G.OpenMore
              }
              
              game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Eggs_RequestPurchase"):InvokeServer(unpack(args))
         end)
      end
   end
end)

spawn(function()
   game:GetService("RunService").RenderStepped:Connect(function()
       if _G.AutoHatch then
            pcall(function()
               game:GetService'VirtualUser':CaptureController()
               game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
           end)
       end
   end)
end)

