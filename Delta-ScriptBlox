	tweenB = tween_sB:Create(game:GetService("Workspace").Boats.Guardian.VehicleSeat, infoB, {CFrame = Pos})
tweenB:Play()
tweenB.Completed:Wait(E)

local tweenfunc = {}
	
function tweenfunc:Stop()
