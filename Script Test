local Dragon_Trident = User:Label("❌: Dragon Trident")
local Yama = User:Label("❌: Yama")
local Buddy_Sword = User:Label("❌: Buddy Sword")
local Canvander = User:Label("❌: Canvander")
local Twin_Hooks = User:Label("❌: Twin Hooks")
