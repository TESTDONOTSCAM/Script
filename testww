function TP1(Pos)
   Distance = (Pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
   if Distance < 25 then
       Speed = 10
   elseif Distance < 50 then
       Speed = 10
   elseif Distance < 150 then
       Speed = 10
   elseif Distance < 250 then
       Speed = 10
   elseif Distance < 500 then
       Speed = 10
   elseif Distance < 750 then
       Speed = 210
   elseif Distance >= 1000 then
       Speed = 40
   end
   game:GetService("TweenService"):Create(
       game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
       TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),
       {CFrame = Pos}
   ):Play()
end

function PetMap()
   if _G.SelectMap == "Area [1]" then
      MapWalk1 = CFrame.new(179.779129, 16.4641533, -176.592178, -0.0198902376, 8.30881746e-08, -0.999802172, -1.81059756e-09, 1, 8.31406339e-08, 0.999802172, 3.46392626e-09, -0.0198902376)
      MapWalk2 = CFrame.new(257.134186, 19.2660294, -231.96579, 0.999987602, 7.46904968e-11, -0.00498521328, -1.5485889e-10, 1, -1.60808504e-08, 0.00498521328, 1.60814224e-08, 0.999987602)

   elseif _G.SelectMap == "Area [2]" then
