--[[

Boss Attack

]]

spawn(function()
	while wait() do
		pcall(function()
		if Auto_Boss then
			if game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false then
				if SelectMonster ~= nil then
					CheckQuestBoss()
					AutoHaki()
					
					repeat wait(.2)
						TelePBoss(CFrameQBoss)
						TP(CFrameQBoss)
						
					until (CFrameQBoss.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 10
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuestBoss, QuestLvBoss)
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
					
					SelectMonster = nil
					_G.Farm_Mon = nil
				else
					CheckQuestBoss()
					AutoHaki()
					
					repeat wait(.2)
						TelePBoss(CFrameQBoss)
						topos(CFrameQBoss)
					until (CFrameQBoss.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 10
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuestBoss, QuestLvBoss)
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
					
				end
			elseif game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
				if _G.Auto_BostyjrtjWs then
					if game.Workspace.Enemies:FindFirstChild(_G.SelectfBoss) then
						for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
							if v.Name == _G.SelectBofss and v.Humanoid.Health > 0 then
								if v.Humanoid:FindFirstChild("Animator") then
									v.Humanoid.Animator:Destroy()
								end
								v.Humanoid:ChangeState(11)
								v.Humanoid.JumpPower = 0
								v.Humanoid.WalkSpeed = 0
								v.HumanoidRootPart.CanCollide = false
								v.HumanoidRootPart.Size = Vector3.new(20,20,20)
								_G.FastAtteack2 = true
								repeat task.wait()
									TelePBoss(CFrameQBowss)
									EquipWeapon(_G.Setting_table.Weapon)
									topos(v.HumanoidRootPart.CFrame * CFrame.new(0,10,0) * Pos)
									game:GetService'VirtualUser':CaptureController()
									game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
								until game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false or not string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, NameBwoss) or not v.Parent or v.Humanoid.Health <= 0 or not Auto_Farm or Mix_Farm
								if not string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, NamewBoss) then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
								end
							end
						end
					else
						topos(CFrameBSOSO * CFrame.new(0,10,0))
						TelePBoss(CFramefeeBoss)
					end
				else
					if game.Workspace.Enemies:FindFirstChild(MsBoss) then
						for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
							if v.Name == MsBoss and v.Humanoid.Health > 0 then
								_G.PosMon = v.HumanoidRootPart.CFrame
								
								if v.Humanoid:FindFirstChild("Animator") then
									v.Humanoid.Animator:Destroy()
								end
								v.Humanoid:ChangeState(11)
								v.Humanoid.JumpPower = 0
								v.Humanoid.WalkSpeed = 0
								v.HumanoidRootPart.CanCollide = false
								v.HumanoidRootPart.Size = Vector3.new(20,20,20)
								repeat task.wait()
									TelePBoss(CFrameQBoss)
									EquipWeapon(_G.Setting_table.Weapon)
									topos(v.HumanoidRootPart.CFrame * CFrame.new(0,10,0) * Pos)
									game:GetService'VirtualUser':CaptureController()
									game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
								until game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false or not string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, NameBoss) or not v.Parent or v.Humanoid.Health <= 0 or not Auto_Farm or Mix_Farm
								if not string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, NameBoss) then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
								end
								Attack = nil
							end
						end
						else
						
						if game.ReplicatedStorage:FindFirstChild(MsBoss) then
							topos(game.ReplicatedStorage:FindFirstChild(MsBoss).HumanoidRootPart.CFrame * CFrame.new(0,10,0))
						else
						topos(CFrameBoss)
						
							end
						end
					end
				end
			end
		end)
	end
end)



